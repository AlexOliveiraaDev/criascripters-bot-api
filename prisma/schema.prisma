generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["debian-openssl-3.0.x"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Words{
  id String @id @default(auto()) @map("_id") @db.ObjectId
  word String
  images String[]
  close_words String[]
  lastDate DateTime
}
